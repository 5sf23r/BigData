<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" 
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
	
	<listener>
		<listener-class>cn.tedu.listener.MySCAtrrListener</listener-class>
	</listener>
	<listener>
		<listener-class>cn.tedu.listener.MyServletRequestListener</listener-class>
	</listener>
	<listener>
		<listener-class>cn.tedu.listener.MyHttpSessionListener</listener-class>
	</listener>
	<listener>
		<listener-class>cn.tedu.listener.MyServletContextListener</listener-class>
	</listener>
  <servlet>
    <servlet-name>SessionTest01</servlet-name>
    <servlet-class>cn.tedu.servlet.SessionTest01</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>SessionTest02</servlet-name>
    <servlet-class>cn.tedu.servlet.SessionTest02</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>SCAttrTest01</servlet-name>
    <servlet-class>cn.tedu.servlet.SCAttrTest01</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>UserSessionTest01</servlet-name>
    <servlet-class>cn.tedu.servlet.UserSessionTest01</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>UserSessionTest02</servlet-name>
    <servlet-class>cn.tedu.servlet.UserSessionTest02</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>UserSessionTest03</servlet-name>
    <servlet-class>cn.tedu.servlet.UserSessionTest03</servlet-class>
  </servlet>






  <servlet-mapping>
    <servlet-name>SessionTest01</servlet-name>
    <url-pattern>/servlet/SessionTest01</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>SessionTest02</servlet-name>
    <url-pattern>/servlet/SessionTest02</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>SCAttrTest01</servlet-name>
    <url-pattern>/servlet/SCAttrTest01</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>UserSessionTest01</servlet-name>
    <url-pattern>/servlet/UserSessionTest01</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>UserSessionTest02</servlet-name>
    <url-pattern>/servlet/UserSessionTest02</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>UserSessionTest03</servlet-name>
    <url-pattern>/servlet/UserSessionTest03</url-pattern>
  </servlet-mapping>
	
  <display-name></display-name>	
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
</web-app>
