<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" 
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
  <display-name></display-name>
  <servlet>
    <servlet-name>CookieDemo1</servlet-name>
    <servlet-class>cn.tedu.cookie.CookieDemo1</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>CookieDemo2</servlet-name>
    <servlet-class>cn.tedu.cookie.CookieDemo2</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>CookieDemo3</servlet-name>
    <servlet-class>cn.tedu.cookie.CookieDemo3</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>CookieDemo4</servlet-name>
    <servlet-class>cn.tedu.cookie.CookieDemo4</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>BuyServlet</servlet-name>
    <servlet-class>cn.tedu.session.BuyServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>PayServlet</servlet-name>
    <servlet-class>cn.tedu.session.PayServlet</servlet-class>
  </servlet>






  <servlet-mapping>
    <servlet-name>CookieDemo1</servlet-name>
    <url-pattern>/servlet/CookieDemo1</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>CookieDemo2</servlet-name>
    <url-pattern>/servlet/CookieDemo2</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>CookieDemo3</servlet-name>
    <url-pattern>/servlet/CookieDemo3</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>CookieDemo4</servlet-name>
    <url-pattern>/servlet/CookieDemo4</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>BuyServlet</servlet-name>
    <url-pattern>/servlet/BuyServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>PayServlet</servlet-name>
    <url-pattern>/servlet/PayServlet</url-pattern>
  </servlet-mapping>	
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
</web-app>
