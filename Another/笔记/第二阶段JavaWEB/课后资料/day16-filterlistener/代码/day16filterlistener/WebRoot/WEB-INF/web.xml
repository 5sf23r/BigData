<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" 
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
  <display-name></display-name>	
  <!-- 配置监听器 -->
  <listener>
  	<listener-class>cn.tedu.listener.SCListener</listener-class>
  </listener>
  
  <listener>
  	<listener-class>cn.tedu.listener.SessionListener</listener-class>
  </listener>
  
  <listener>
  	<listener-class>cn.tedu.listener.MyRequestListener</listener-class>
  </listener>
  
   <listener>
  	<listener-class>cn.tedu.listener.SCAListener</listener-class>
  </listener>
  <filter>
  	<filter-name>FirstFilter</filter-name>
  	<filter-class>cn.tedu.filter.FirstFilter</filter-class>
  </filter>
  <filter>
  	<filter-name>SecondFilter</filter-name>
  	<filter-class>cn.tedu.filter.SecondFilter</filter-class>
  </filter>
  
  
  
  <filter-mapping>
  	<filter-name>FirstFilter</filter-name>
  	<url-pattern>/*</url-pattern>
  </filter-mapping>
  <filter-mapping>
  	<filter-name>SecondFilter</filter-name>
  	<url-pattern>/*</url-pattern>
  </filter-mapping>
  <servlet>
    <servlet-name>FirstServlet</servlet-name>
    <servlet-class>cn.tedu.filter.FirstServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>Demo1</servlet-name>
    <servlet-class>cn.tedu.listener.Demo1</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>Demo2</servlet-name>
    <servlet-class>cn.tedu.listener.Demo2</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>Demo3</servlet-name>
    <servlet-class>cn.tedu.listener.Demo3</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>Demo4</servlet-name>
    <servlet-class>cn.tedu.listener.Demo4</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>Demo5</servlet-name>
    <servlet-class>cn.tedu.listener.Demo5</servlet-class>
  </servlet>






  <servlet-mapping>
    <servlet-name>FirstServlet</servlet-name>
    <url-pattern>/servlet/FirstServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Demo1</servlet-name>
    <url-pattern>/servlet/Demo1</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Demo2</servlet-name>
    <url-pattern>/servlet/Demo2</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Demo3</servlet-name>
    <url-pattern>/servlet/Demo3</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Demo4</servlet-name>
    <url-pattern>/servlet/Demo4</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Demo5</servlet-name>
    <url-pattern>/servlet/Demo5</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
</web-app>
